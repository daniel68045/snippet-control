{
  "name": "vscode-snippet-gen",
  "displayName": "Snippet Control",
  "description": "Generate and manage reusable code snippets in Visual Studio Code.",
  "version": "1.0.1",
  "publisher": "DanielPupke",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Snippets"
  ],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel68045/snippet-ctrl"
  },
  "bugs": {
    "url": "https://github.com/daniel68045/snippet-ctrl/issues"
  },
  "homepage": "https://github.com/daniel68045/snippet-ctrl",
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snippet-gen.generateSnippet",
        "title": "Generate Code Snippet"
      },
      {
        "command": "snippet-gen.deleteSnippet",
        "title": "Delete Snippet"
      },
      {
        "command": "snippet-gen.importSnippet",
        "title": "Import External Snippets"
      },
      {
        "command": "snippet-gen.exportSnippet",
        "title": "Export Internal Snippets"
      }
    ]
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@vscode/vsce": "^2.14.0"
  }
}
