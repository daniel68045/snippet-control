{
  "name": "vscode-snippet-gen",
  "displayName": "Snippet Generator",
  "description": "Generate and manage reusable code snippets in VS Code.",
  "version": "0.1.0",
  "publisher": "daniel68045",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Extension Packs"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snippet-gen.generateSnippet",
        "title": "Generate Code Snippet"
      },
      {
        "command": "snippet-gen.deleteSnippet",
        "title": "Delete Snippet"
      },
      {
        "command": "snippet-gen.importSnippet",
        "title": "Import External Snippets"
      },
      {
        "command": "snippet-gen.exportSnippet",
        "title": "Export Internal Snippets"
      }
    ]
  },
  "scripts": {
    "build": "echo 'No build steps required'",
    "test": "node ./test/extension.test.js"
  },
  "devDependencies": {}
}
